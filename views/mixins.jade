//- Navbar mixins
mixin nav(name, id, style)
 - var style = (typeof style === 'undefined') ? "default" : style
 nav( role="navigation", class=["navbar", "navbar-" + style, "navbar-custom"] )
   div.navbar-inner
     img.icon(src='/images/icon.png')
     a.headerTitle(href="/")= name
     .adminStuff
       .adiv
         a(href='/admin/set')
           img(src='/images/super.png')
           p Log in as Admin
        .adiv
         a(href='/admin/remove')
           img(src='/images/logout.png')
           p Log out of admin
